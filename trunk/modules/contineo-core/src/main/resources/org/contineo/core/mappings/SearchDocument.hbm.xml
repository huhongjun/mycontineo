<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="org.contineo.core.searchengine.SearchDocument" table="co_searchdocument">
		<cache usage="read-write" />
		<id name="luceneId" type="int" column="co_luceneid" />
		<property name="menuId" type="int" column="co_menuid" />
		<property name="index" type="string" column="co_index" length="10" />
	</class>
</hibernate-mapping>
